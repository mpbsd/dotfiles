snippet paper "projections for a scientific paper" bA
{
  "makefile": {
    "skeleton": "latex",
    "type": "makefile"
  },
  "main.tex": {
    "alternate": "sec/01.tex",
    "skeleton": "article",
    "type": "article"
  },
  "sec/01.tex": {
    "alternate": "sec/02.tex",
    "skeleton": "introduction",
    "type": "introduction"
  },
  "sec/02.tex": {
    "alternate": "sec/03.tex",
    "skeleton": "results",
    "type": "results"
  },
  "sec/03.tex": {
    "alternate": "sec/04.tex",
    "skeleton": "examples",
    "type": "examples"
  },
  "sec/04.tex": {
    "alternate": "main.tex",
    "skeleton": "proofs",
    "type": "proofs"
  },
  "ref/main.bib": {
    "type": "bibliography"
  }
}
endsnippet

snippet meta "metadata for classnotes" bA
`!p
from datetime import datetime
date = datetime.now().strftime("%Y-%m-%d")
`{
  "metadata": {
    "topic": "$1",
    "date": "${2:`!p snip.rv = date`}"
  },
  "reference": {
    "title": "$3",
    "author": "$4",
    "key": "$5"
  }$0
}
endsnippet
